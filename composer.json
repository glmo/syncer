{
  "name": "goce/Syncer",
  "description": "Synder is a command line tool that provides a way to sync Drupal and ExpressionEngine databases from local to remote servers via ssh",
  "license": "MIT",
  "keywords": ["syncer, ssh"],
  "authors": [
    {
      "name": "Goce Evtimovski",
      "email": "goce@lmo.com"
    }
  ],
  "require": {
    "php": ">=5.5.3",
    "illuminate/remote" : "dev-master",
    "symfony/console" : "dev-master",
    "symfony/process" : "dev-master",
    "symfony/yaml": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*",
    "mikey179/vfsStream": "~1"
  },
  "autoload": {
    "classmap": [
      "src/",
      "src/Credential"
    ],
    "psr-4": {
      "Syncer\\": "src/"
    }
  }
}
